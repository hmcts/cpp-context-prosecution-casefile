{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://cpp.moj.gov.uk/prosecution/casefile/json/schemas/licence-code-reference-data.json",
  "type": "object",
  "properties": {
    "id": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "sequence": {
      "type": "integer"
    },
    "licenceCode": {
      "type": "string",
      "maxLength": 10
    },
    "licenceCodeDescription": {
      "type": "string",
      "maxLength": 255
    },
    "validFrom": {
      "type": "string",
      "format": "date"
    }
  },
  "required": [
    "id",
    "sequence",
    "licenceCode",
    "licenceCodeDescription"
  ],
  "additionalProperties": false
}