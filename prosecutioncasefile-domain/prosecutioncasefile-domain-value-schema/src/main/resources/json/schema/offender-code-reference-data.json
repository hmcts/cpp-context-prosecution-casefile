{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://cpp.moj.gov.uk/prosecution/casefile/json/schemas/offender-code-reference-data.json",
  "type": "object",
  "properties": {
    "id": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "seqNum": {
      "type": "integer"
    },
    "offenderCode": {
      "type": "string"
    },
    "offenderCodeDescription": {
      "type": "string"
    },
    "validFrom": {
      "type": "string",
      "format": "date"
    }
  },
  "required": [
    "id",
    "seqNum",
    "offenderCode",
    "offenderCodeDescription"
  ]
}