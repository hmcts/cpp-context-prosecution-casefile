    "cmsDocumentIdentifier": {
      "$ref": "http://cpp.moj.gov.uk/prosecution/casefile/json/schemas/cms-document-identifier.json"
    },
    "material": {
      "$ref": "http://cpp.moj.gov.uk/prosecution/casefile/json/schemas/material.json"
    },
    "receivedDateTime": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/date-time"
    },
    "defendantName": {
      "type": "string"
