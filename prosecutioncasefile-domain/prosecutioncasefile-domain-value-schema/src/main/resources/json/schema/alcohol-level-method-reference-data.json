{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://cpp.moj.gov.uk/prosecution/casefile/json/schemas/alcohol-level-method-reference-data.json",
  "type": "object",
  "properties": {
    "id": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "seqNo": {
      "type": "integer"
    },
    "methodCode": {
      "type": "string"
    },
    "methodDescription": {
      "type": "string"
    },
    "validFrom": {
      "type": "string",
      "format": "date"
    },
    "validTo": {
      "type": "string",
      "format": "date"
    }
  },
  "required": [
    "id",
    "seqNo",
    "methodCode"
  ]
}
