    },
    "contactEmailAddress": {
      "type": "string"
    },
    "oucode": {
      "type": "string"
    },
    "spiInFlag": {
      "type": "boolean"
    },
    "spiOutFlag": {
      "type": "boolean"
    },
    "sjpFlag": {
      "type": "boolean"
    },
    "cpsFlag": {
      "type": "boolean"
    },
    "informantEmailAddress": {
      "type": "string"
    },
    "prosecutorCategory": {
      "type": "string"
    },
    "standard": {
      "type": "boolean"
    },
