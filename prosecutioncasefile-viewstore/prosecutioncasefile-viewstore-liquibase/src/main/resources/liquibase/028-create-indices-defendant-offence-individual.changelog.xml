<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">
    <changeSet id="028" author="prosecutioncasefile"
               logicalFilePath="028-create-indices-defendant-offence-individual.changelog.xml">
        <createIndex tableName="defendant"
                     indexName="defendant_index_case_id_idx">
            <column name="case_id"/>
        </createIndex>
        <createIndex tableName="offence"
                     indexName="offence_index_defendant_id_idx">
            <column name="defendant_id"/>
        </createIndex>
        <createIndex tableName="defendant_individual_aliases"
                     indexName="defendant_individual_aliases_index_defendant_id_idx">
            <column name="defendant_id"/>
        </createIndex>
    </changeSet>
</databaseChangeLog>