{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://moj.gov.uk/cps/prosecutioncasefile/domain/event/group-cases-parked-for-approval.json",
  "type": "object",
  "properties": {
    "applicationId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "groupProsecutionList": {
      "type": "object",
      "properties": {
        "groupProsecutionWithReferenceDataList": {
          "type": "array",
          "minItems": 1,
          "items": {
            "type": "object",
            "properties": {
               "groupProsecution": {
                 "$ref": "http://cpp.moj.gov.uk/prosecution/casefile/json/schemas/group-prosecution.json"
               }
            },
            "required": [
              "groupProsecution"
            ]
          }
        },
        "channel": {
          "type": "string",
          "$ref": "http://cpp.moj.gov.uk/prosecution/casefile/json/schemas/pcf-definitions.json#/definitions/channel"
        },
        "externalId": {
          "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid",
          "description": "ID originating from the client (Civil) for referencing the original message"
        }
      },
      "required": [
        "groupProsecutionWithReferenceDataList",
        "externalId",
        "channel"
      ]
    },
    "defendantWarnings": {
      "type": "array",
      "items": {
        "$ref": "http://cpp.moj.gov.uk/prosecution/casefile/json/schemas/defendant-problem.json"
      }
    }
  },
  "required": [
    "applicationId",
    "groupProsecutionList"
  ]
}