{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://moj.gov.uk/cpp/json/schemas/prosecutioncasefile/events/defendants-parked-for-summons-application-approval.json",
  "type": "object",
  "properties": {
    "applicationId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "prosecutionWithReferenceData": {
      "type": "object",
      "properties": {
        "prosecution": {
          "$ref": "http://cpp.moj.gov.uk/prosecution/casefile/json/schemas/prosecution.json"
        }
      }
    },
    "defendantWarnings": {
      "type": "array",
      "items": {
        "$ref": "http://cpp.moj.gov.uk/prosecution/casefile/json/schemas/defendant-problem.json"
      }
    }
  },
  "required": [
    "applicationId",
    "prosecutionWithReferenceData"
  ]
}